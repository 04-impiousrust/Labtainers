logger = .bash_history : STRING_COUNT : logger
last = /root/.bash_history : STRING_COUNT : last
service = /root/.bash_history : STRING_COUNT : service rsyslog restart
mark = /var/log/syslog : CONTAINS : MARK
_debug = /var/log/mydebug : CONTAINS : sys-log student
_service_in_debug = /var/log/mydebug : CONTAINS : Stopping System Logging
